//User function Template for C++

queue<int>_push(int arr[],int n)
{
   queue<int> res;
   for(int i = 0; i < n; i++){
       res.push(arr[i]);
   }
   return res;
}

void _pop(queue<int>s)
{
    //print front and dequeue for each element until it becomes empty
    while(!s.empty()){
        cout << s.front() << " ";
        s.pop();
    }
} 
